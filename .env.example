# Configuración de APIs de Envío
# Copia este archivo a .env y completa con tus credenciales reales

# Uber Direct API
UBER_DIRECT_CLIENT_ID=tu_client_id_aqui
UBER_DIRECT_CLIENT_SECRET=tu_client_secret_aqui
UBER_DIRECT_SANDBOX=true

# Andreani API
ANDREANI_API_KEY=tu_api_key_aqui
ANDREANI_CLIENT_ID=tu_client_id_aqui
ANDREANI_CLIENT_SECRET=tu_client_secret_aqui
ANDREANI_SANDBOX=true

# Webhook Security
WEBHOOK_SECRET=tu_webhook_secret_super_seguro_aqui

# Debug y Logs
SHIPPING_DEBUG=true

# URLs de Webhook (para configurar en las APIs)
# Uber Direct: https://tu-dominio.com/proyecto/webhook/uber-direct.php
# Andreani: https://tu-dominio.com/proyecto/webhook/andreani.php